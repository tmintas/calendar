{
  "compileOnSave": false,
  "compilerOptions": {
    "allowUnusedLabels": false,
    "baseUrl": "./src",
    "paths": {
      "@extensions/*": ["app/extensions/*.ts"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",    
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "noUnusedLocals": true
  }
}
